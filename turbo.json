{
 "$schema": "./node_modules/turbo/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "inputs": ["**/*.{ts,tsx}"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
